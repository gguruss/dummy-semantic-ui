{
  "base": "semantic\\",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "..\\dist\\semantic\\",
      "uncompressed": "..\\dist\\semantic\\components\\",
      "compressed": "..\\dist\\semantic\\components\\",
      "themes": "..\\dist\\semantic\\themes\\"
    },
    "clean": "dist/"
  },
  "permission": 744,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "rail",
    "reveal",
    "segment",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "ad",
    "card",
    "item",
    "statistic",
    "accordion",
    "dimmer",
    "dropdown",
    "embed",
    "modal",
    "nag",
    "shape",
    "sidebar",
    "sticky",
    "transition",
    "form",
    "state",
    "visibility"
  ],
  "version": "2.2.6"
}